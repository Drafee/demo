  tag: Scene0_1
  lines:
  - speaker: Audience
    text: "\u5979\u660F\u8FF7\u5DF2\u7ECF\u4E09\u5929\u4E86\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: Audience
    text: "\"\u533B\u751F\uFF0C\u60A8\u662F\u6211\u4EEC\u6700\u540E\u7684\u5E0C\u671B\""
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u4E0D\u662F\u666E\u901A\u7684\u533B\u751F\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u662F\u4E2A\u50AC\u7720\u5E08\uFF0C\u4E13\u95E8\u8FDB\u5165\u522B\u4EBA\u7684\u6F5C\u610F\u8BC6\uFF0C\u628A\u4ED6\u4EEC\u4ECE\u8BB0\u5FC6\u7684\u6DF1\u6E0A\u91CC\u62C9\u51FA\u6765\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u8981\u505A\u7684\uFF0C\u662F\u6536\u96C6\u90A3\u4E9B\u88AB\u538B\u6291\u3001\u88AB\u85CF\u8D77\u6765\u7684\u58F0\u97F3\u788E\u7247\uFF0C\u628A\u5B83\u4EEC\u62FC\u56DE\u539F\u6765\u7684\u6837\u5B50\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u6BCF\u4E00\u6BB5\u8BB0\u5FC6\u7684\u80CC\u540E\uFF0C\u90FD\u85CF\u7740\u4E00\u70B9\u70B9\u7EBF\u7D22\uFF0C\u6307\u5411\u4E00\u4E2A\u5979\u5FEB\u8981\u5FD8\u8BB0\u7684\u6E29\u6696\u65F6\u523B\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u5F97\u627E\u5230\u5B83\u4EEC\uFF0C\u62FC\u5BF9\u5B83\u4EEC\uFF0C\u7136\u540E\u2014\u2014\u5E26\u5979\u56DE\u6765\u3002"
    audioTag: 
    typingSpeed: 0
  - speaker: I
    text: "\u5F00\u59CB\u5427"
    audioTag: 
    typingSpeed: 0
