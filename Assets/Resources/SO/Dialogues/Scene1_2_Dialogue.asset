  lines:
  - speaker: I
    text: "\u4ED6\u4EEC\u53C8\u5435\u67B6\u4E86\u3002"
    audioTag: 2-1
    typingSpeed: 0
  - speaker: I
    text: "\u58F0\u97F3\u6BD4\u4E0A\u4E00\u6B21\u8FD8\u5927\u3002"
    audioTag: 2-1
    typingSpeed: 0
  - speaker: I
    text: "\u7535\u89C6\u8FD8\u5728\u653E\u7740\u4EC0\u4E48\uFF0C\u4F46\u6211\u542C\u4E0D\u592A\u6E05\uFF0C"
    audioTag: 2-1
    typingSpeed: 0
  - speaker: I
    text: "\u6BCF\u6B21\u8FD9\u79CD\u65F6\u5019"
    audioTag: 2-2
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u4F1A\u5C3D\u91CF\u4E0D\u52A8\uFF0C\u5C3D\u91CF\u4E0D\u51FA\u58F0\u3002"
    audioTag: 2-2
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u77E5\u9053\u63A5\u4E0B\u6765\u4F1A\u6709\u4EC0\u4E48\u2014\u2014"
    audioTag: 2-2
    typingSpeed: 0
  - speaker: I
    text: "\u676F\u5B50\u7838\u788E\uFF0C\u9065\u63A7\u5668\u7529\u51FA\u53BB\uFF0C\u8FD8\u6709\u95E8\uFF0C\u201C\u7830\u201D\u4E00\u58F0\u3002"
    audioTag: 2-2
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u5DF2\u7ECF\u731C\u5F97\u51FA\u987A\u5E8F\u4E86\u3002"
    audioTag: 2-3
    typingSpeed: 0
  - speaker: I
    text: "\u6BCF\u6B21\u8FD9\u79CD\u65F6\u5019"
    audioTag: 2-3
    typingSpeed: 0
  - speaker: I
    text: "\u6211\u90FD\u4F1A\u6342\u4F4F\u81EA\u5DF1\u7684\u8033\u6735\uFF0C\u80FD\u542C\u5230\u81EA\u5DF1\u7684\u5FC3\u8DF3\u3002"
    audioTag: 2-3
    typingSpeed: 0
  - speaker: I
    text: "\u6709\u65F6\u5019\u5F1F\u5F1F\u4F1A\u8DD1\u8FDB\u6765\u770B\u6211\uFF0C"
    audioTag: 2-4
    typingSpeed: 0
  - speaker: I
    text: "\u4ED6\u4F1A\u62CD\u62CD\u6211\uFF0C\u8BF4\uFF1A\u201C\u522B\u6015\u4E0D\u662F\u4F60\u7684\u9519\u3002\u201D"
    audioTag: 2-4
    typingSpeed: 0
  - speaker: I
    text: "\u90A3\u4E00\u523B\uFF0C\u597D\u50CF\u4EC0\u4E48\u90FD\u5B89\u9759\u4E86\u4E00\u4E0B\u3002"
    audioTag: 2-4
    typingSpeed: 0
